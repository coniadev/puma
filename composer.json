{
    "name": "conia/puma",
    "description": "A 'no ORM' database library which uses plain SQL files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ebene fÃ¼nf GmbH",
            "email": "conia@ebenefuenf.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://conia.dev/puma",
    "keywords": [
        "conia",
        "database",
        "sql"
    ],
    "autoload": {
        "psr-4": {
            "Conia\\Puma\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conia\\Puma\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "conia/cli": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^1",
        "vimeo/psalm": "^4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "staabm/annotate-pull-request-from-checkstyle": "^1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
